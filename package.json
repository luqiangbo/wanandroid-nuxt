{
  "name": "web-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:v1": "cross-env MY_API_ENV=local nuxt --config-file v1/nuxt.config.js",
    "dev-l:v1": "nuxt --config-file v1/nuxt.config.js",
    "dev-t:v1": "cross-env MY_API_ENV=test nuxt --config-file v1/nuxt.config.js",
    "dev-p:v1": "cross-env MY_API_ENV=production nuxt --config-file v1/nuxt.config.js",
    "build:v1": "nuxt build --config-file v1/nuxt.config.js",
    "start:v1": "nuxt start --config-file v1/nuxt.config.js",
    "cx:v1": "npm run build:v1 && npm run start:v1",
    "dev:v2": "nuxt --config-file v2/nuxt.config.js",
    "build:v2": "nuxt build --config-file v2/nuxt.config.js",
    "start:v2": "nuxt start --config-file v2/nuxt.config.js",
    "cx:v2": "npm run build:v2 && npm run start:v2",
    "all": "npm-run-all --parallel cx:*",
    "all-start": "npm-run-all --parallel start:*",
    "rimraf": "rimraf node_modules",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@nuxt/content": "^1.5.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "element-ui": "^2.13.2",
    "nuxt": "^2.14.0",
    "swiper": "^5.4.0",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.3"
  }
}
